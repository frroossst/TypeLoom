{
	"name": "typeloom",
	"displayName": "TypeLoom",
	"description": "adding optional and graudal types to Suneido",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.81.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "suneido",
				"aliases": [
					"Suneido",
					"suneido"
				],
				"extensions": [
					".su"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "suneido",
				"scopeName": "main.su",
				"path": "./syntaxes/suneido.tmLanguage.json"
			}
		]
	},
	"devDependencies": {
		"@types/node": "^20.5.6"
	},
	"dependencies": {
		"@types/vscode": "^1.81.0",
		"vscode": "^1.1.37",
		"vscode-languageclient": "^8.1.0",
		"vscode-languageserver": "^8.1.0",
		"vscode-languageserver-textdocument": "^1.0.8"
	}
}
