{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Suneido",
	"fileTypes": ["*.su", "*.suneido"],
	"patterns": [
		{
			"include": "#comments"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#variables"
		}
	],
	"repository": {
		"comments": {
			"patterns": [
				{
					"name": "comment.line.suneido",
					"match": "//.*$"
				},
				{
					"name": "comment.block.suneido",
					"begin": "/\\*",
					"end": "\\*/"
				}
			]
		},
		"keywords": {
			"patterns": [{
				"name": "keyword.control.suneido",
				"match": "\\b(and|break|callback|catch|class|case|continue|default|dll|do|else|false|for|foreach|forever|function|if|in|is|isnt|not|or|return|struct|super|switch|this|throw|true|try|while)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.suneido",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.suneido",
					"match": "\\\\."
				}
			]
		},
		"variables": {
			"patterns": [
				{
					"name": "variable.other.suneido",
					"match": "\\b([A-Z][a-zA-Z0-9]*)\\b"
				}
			]
		}
	},
	"scopeName": "main.su"
}
